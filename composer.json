{
  "require": {
    "craftcms/cms": "^4.0.0",
    "craftcms/feed-me": "5.0.4",
    "craftcms/redactor": "3.0.0",
    "nystudio107/craft-imageoptimize": "4.0.0-beta.3",
    "nystudio107/craft-seomatic": "4.0.1",
    "putyourlightson/craft-blitz": "4.1.3",
    "solspace/craft-freeform": "4.0.0-beta.9",
    "topshelfcraft/wordsmith": "4.0.0.1",
    "vaersaagod/dospaces": "2.0.0",
    "verbb/navigation": "2.0.1",
    "verbb/vizy": "2.0.1",
    "vlucas/phpdotenv": "^3.4.0",
    "wbrowar/adminbar": "3.2.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
